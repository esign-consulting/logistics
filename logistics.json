{
    "id": "/logistics",
    "acceptedResourceRoles": [
        "slave_public"
    ],
    "instances": 2,
    "cpus": 1,
    "mem": 1024,
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "esignbr/logistics"
        },
        "portMappings": [
            {
                "containerPort": 8080,
                "hostPort": 0,
                "servicePort": 10105
            }
        ]
    },
    "env": {
        "MONGODB_URI": "mongodb://mongodb.marathon.l4lb.thisdcos.directory/logistics"
    },
    "labels": {
        "HAPROXY_GROUP": "external",
        "HAPROXY_DEPLOYMENT_GROUP": "logistics"
    },
    "healthChecks": [
        {
            "gracePeriodSeconds": 300,
            "intervalSeconds": 30,
            "maxConsecutiveFailures": 3,
            "path": "/logistics",
            "protocol": "HTTP",
            "timeoutSeconds": 30,
            "delaySeconds": 15
        }
    ],
    "networks": [
        {
            "mode": "container/bridge"
        }
    ]
}